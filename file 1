x=1
y=2.5

#string
prenon ="margheni"

#affectation
print(x+y)
print(x-y)
print(x*y)
print(x/y)

#comparisaon
print(x<=y)
print(x>=y)
print(x==y)
print(x!=y)

#lgique
print(False & True) # AND ET
print(False | True) # OR OU
print(False ^ True) # XOR
print(True ^ True) # XOR
#fonction f(x)
f=lambda x,y: x**2+y
print(f(3,2))

#fonction
def e_potentielle (masse, hauteur,g):
    E = masse *hauteur *g 
    return(E)
k=e_potentielle(2,3,6.77)
print(k)
